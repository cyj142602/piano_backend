CREATE TABLE "PIANO_BACKEND"."PIANO_STUDENT"
(
"ID" VARCHAR2(50) NOT NULL,
"PIANO_USER_ID" VARCHAR2(50) NOT NULL,
"STUDENT_NAME" VARCHAR2(50) NOT NULL,
"GENDER" INTEGER NOT NULL,
"AGE" INTEGER,
"PHOTO" VARCHAR2(50),
"CREATE_TIME" DATETIME(6),
"UPDATE_TIME" DATETIME(6),
"DEL_FLAG" INTEGER DEFAULT 0 NOT NULL,
"NICK_NAME" VARCHAR2(50),
"BIRTHDAY" DATETIME(6),
"CALENDAR" INTEGER,
CLUSTER PRIMARY KEY("ID")) STORAGE(ON "MAIN", CLUSTERBTR) ;

COMMENT ON TABLE "PIANO_BACKEND"."PIANO_STUDENT" IS '?生表';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_STUDENT"."PHOTO" IS '照片';

CREATE TABLE "PIANO_BACKEND"."PIANO_USER"
(
"ID" VARCHAR2(50) NOT NULL,
"OPEN_ID" VARCHAR2(50) NOT NULL,
"NICK_NAME" VARCHAR2(50),
"GENDER" INTEGER,
"PHONE" VARCHAR2(50),
"PERMISSION" INTEGER,
"CREATE_TIME" DATETIME(6),
"UPDATE_TIME" DATETIME(6),
"DEL_FLAG" INTEGER DEFAULT 0 NOT NULL,
CLUSTER PRIMARY KEY("ID")) STORAGE(ON "MAIN", CLUSTERBTR) ;

COMMENT ON TABLE "PIANO_BACKEND"."PIANO_USER" IS '用?表';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_USER"."ID" IS '主?';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_USER"."OPEN_ID" IS 'openID';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_USER"."NICK_NAME" IS '??';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_USER"."GENDER" IS '性?，1男2女';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_USER"."PHONE" IS '手机?';
COMMENT ON COLUMN "PIANO_BACKEND"."PIANO_USER"."PERMISSION" IS '?限，1?客2正式用?';
